{
  "name": "@blockstack/stacks-transactions",
  "version": "0.3.0-alpha.3",
  "description": "Javascript library for constructing transactions on the Stacks blockchain.",
  "main": "lib/src/index",
  "types": "lib/src/index.d.ts",
  "unpkg": "dist/stacks-transactions.js",
  "scripts": {
    "build": "npm run build:node",
    "build:webpack": "shx rm -rf dist/* && cross-env NODE_ENV=production webpack --mode=production",
    "build:webpack:watch": "shx rm -rf dist/* && cross-env NODE_ENV=development webpack --mode=development --watch --progress --info-verbosity verbose",
    "build:webpack:analyze": "shx rm -rf dist/* && cross-env NODE_ENV=production ANALYZE=true webpack --mode=production",
    "build:docs": "rimraf docs && cross-env typedoc --name \"stacks-transactions-js $npm_package_version Library Reference\" --tsconfig tsconfig.typedoc.json --out docs --json docs/docs.json src",
    "start": "webpack-dev-server",
    "build:node": "shx rm -rf lib && tsc --declaration",
    "build:node:watch": "shx rm -rf lib && tsc --watch",
    "lint": "eslint ./src ./tests --ext .ts",
    "lint:fix": "eslint ./src ./tests --ext .ts --fix",
    "test": "jest --config ./jest.config.js --coverage",
    "prepublishOnly": "npm run test && npm run build",
    "postinstall": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockstack/stacks-transactions-js.git"
  },
  "author": {
    "name": "Blockstack PBC",
    "email": "admin@blockstack.com",
    "url": "https://blockstack.com"
  },
  "contributors": [
    {
      "name": "Ken Liao"
    },
    {
      "name": "Reed Rosenbluth"
    },
    {
      "name": "Matthew Little"
    }
  ],
  "license": "GPL-3.0",
  "devDependencies": {
    "@blockstack/eslint-config": "^1.0.2",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.0",
    "babel-loader": "^8.0.6",
    "core-js": "^3.2.1",
    "cross-env": "^5.2.1",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "jest-fetch-mock": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "shx": "^0.3.2",
    "source-map-support": "^0.5.16",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.1.0",
    "ts-node": "^8.6.2",
    "typedoc": "^0.16.11",
    "typescript": "^3.6.3",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "@types/bn.js": "^4.11.6",
    "@types/elliptic": "^6.4.12",
    "@types/lodash": "^4.14.149",
    "@types/randombytes": "^2.0.0",
    "@types/ripemd160": "^2.0.0",
    "@types/sha.js": "^2.4.0",
    "bn.js": "^4.11.8",
    "c32check": "^1.0.1",
    "cross-fetch": "^3.0.4",
    "elliptic": "^6.5.2",
    "lodash": "^4.17.15",
    "randombytes": "^2.1.0",
    "ripemd160": "^2.0.2",
    "sha.js": "^2.4.11",
    "smart-buffer": "^4.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "nyc": {
    "all": true,
    "cache": false,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ]
  }
}
